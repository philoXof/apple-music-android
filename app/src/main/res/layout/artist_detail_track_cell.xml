<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_track_artDetail_cell"
        android:layout_width="match_parent"
        android:layout_height="90dp">


        <TextView
            android:id="@+id/artDetail_track_number"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_margin="15dp"

            app:layout_constraintTop_toTopOf="@id/constraint_track_artDetail_cell"
            app:layout_constraintLeft_toLeftOf="@id/constraint_track_artDetail_cell"/>



        <TextView
            android:id="@+id/artDetail_track_title"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_margin="30dp"

            app:layout_constraintTop_toTopOf="@id/constraint_track_artDetail_cell"
            app:layout_constraintLeft_toRightOf="@id/artDetail_track_number"/>



    </androidx.constraintlayout.widget.ConstraintLayout>


</LinearLayout>